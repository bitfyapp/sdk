{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "test",
          "build",
          "docfy"
        ]
      }
    }
  },
  "targetDefaults": {
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "docfy": {
      "dependsOn": [
        "^docfy"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
